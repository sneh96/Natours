{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node node_modules/node-sass/bin/node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node node_modules/node-sass/bin/node-sass sass/main.scss css/style.comp.css",
    "concat:css": "node node_modules/concat concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": " node node_modules/postcss postcss css/style.concat.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "node node_modules/node-sass css/style.prefix.css css/style.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sneh96/Natours.git"
  },
  "author": "Snehal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sneh96/Natours/issues"
  },
  "homepage": "https://github.com/sneh96/Natours#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0"
  }
}
